{
  "name": "bookmark",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha ./tests/**/*spec.js -- --ui bdd -R spec -t 5000"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "crypto": "^1.0.1",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "node-webshot": "^1.0.3",
    "test": "^0.6.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-html": "^1.6.0",
    "eslint-plugin-import": "^2.7.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "rewire": "^4.0.1",
    "sinon": "^4.0.0",
    "sinon-chai": "^2.14.0",
    "snyk": "^1.34.3",
    "sqlite3": "^4.0.2",
    "standard": "^10.0.2"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "plugins": [
      "html",
      "chai-friendly"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "func-names": [
        "error",
        "never"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "__set__",
            "__get__",
            "__with__"
          ]
        }
      ],
      "no-unused-expressions": 0,
      "chai-friendly/no-unused-expressions": 2
    },
    "settings": {
      "html/indent": "+2"
    },
    "globals": {
      "suite": false,
      "test": false,
      "fixture": false,
      "assert": false,
      "it": false,
      "describe": false,
      "ERRORS": true
    }
  }
}
